{
  "name": "dockerdrupal/cli",
  "description": "DockerDrupal CLI",
  "keywords": ["Drupal", "Docker", "Console", "Development", "Symfony"],
  "homepage": "https://www.4alldigital.io/docker-drupal/",
  "license": "MIT",
  "type": "project",
  "require": {
    "symfony/finder": "~3.0",
    "symfony/console": "~3.0",
    "symfony/yaml": "~3.0",
    "symfony/filesystem": "~3.0",
    "symfony/process": "~3.0",
    "symfony/event-dispatcher": "~3.0",
    "symfony/dependency-injection": "~3.0",
    "symfony/http-kernel": "~3.0",
    "symfony/class-loader": "~3.0",
    "symfony/config": "~3.0",
    "symfony/var-dumper": "~3.0",
    "symfony/framework-bundle": "~3.0",
    "stecman/symfony-console-completion": "^0.5.1",
    "php": "^5.5",
    "doctrine/cache": "^1.6",
    "guzzlehttp/guzzle": "^6.2",
    "guzzlehttp/ringphp": "^1.1",
    "psr/log": "^1.0.0",
    "padraic/phar-updater": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.3 <4.8",
    "drush/drush": "^8.0",
    "sensiolabs/security-checker": "^3.0",
    "symfony/debug": "~3.0"
  },
  "authors": [
    {
      "name": "Joe Mewes"
    }
  ],
  "bin": ["bin/dockerdrupal"],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Docker\\Drupal\\": "src"
    }
  }
}
